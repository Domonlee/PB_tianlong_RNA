<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/temp_rl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true">
        <org.tianlong.rna.utlis.TimeWheelView
	        android:id="@+id/temp_thousand"
	        android:layout_width="70dip"
	        android:layout_height="220dip"/>
        <org.tianlong.rna.utlis.TimeWheelView
	        android:id="@+id/temp_hundred"
	        android:layout_width="70dip"
	        android:layout_height="220dip"
	        android:layout_marginLeft="30dip"
	        android:layout_toRightOf="@id/temp_thousand"/>
        <org.tianlong.rna.utlis.TimeWheelView
	        android:id="@+id/temp_ten"
	        android:layout_width="70dip"
	        android:layout_height="220dip"
	        android:layout_marginLeft="30dip"
	        android:layout_toRightOf="@id/temp_hundred"/>
    </RelativeLayout>
</RelativeLayout>
