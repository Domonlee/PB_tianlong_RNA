<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#505E8D">
    <RelativeLayout android:id="@+id/experiment_new_main_item_rl"
        android:layout_width="344dip"
        android:layout_height="500dip"
        android:layout_marginRight="10dip"
    	android:background="@android:color/white">
    	<RelativeLayout android:id="@+id/experiment_new_main_item_bottom_rl"
	        android:layout_width="fill_parent"
	        android:layout_height="43dip"
	        android:layout_marginLeft="2dip"
	        android:layout_marginRight="2dip"
	        android:background="#314F67">
	        <EditText android:id="@+id/experiment_new_main_item_bottom_name_et"
	            android:layout_width="fill_parent"
	            android:layout_height="wrap_content"
	            android:text="裂     解"
	            android:textSize="25sp"
	            android:textColor="@android:color/white"
	            android:background="#314F67"
	            android:gravity="center"
	            android:layout_centerInParent="true"
	            android:singleLine="true"
	            android:digits="abcdefghijklmnoporstuvwxyzABCDEFGHIGKLMNOPQRSTUVWXYZ1234567890_"/>
	    </RelativeLayout>
	    <RelativeLayout android:id="@+id/experiment_new_main_item_head_rl"
	        android:layout_width="fill_parent"
	        android:layout_height="168dip"
	        android:layout_margin="2dip"
	        android:background="#314F67"
	        android:layout_below="@id/experiment_new_main_item_bottom_rl">
	        <RelativeLayout android:id="@+id/experiment_new_main_item_head_wait_rl"
	            android:layout_width="wrap_content"
	            android:layout_height="54dip"
	            android:layout_centerHorizontal="true">
	            <TextView android:id="@+id/experiment_new_main_item_head_wait_tv"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:text="@string/exp_wait"
	                android:textSize="20sp"
	                android:textColor="@android:color/white"
	                android:layout_centerVertical="true"/>
	            <TextView android:id="@+id/experiment_new_main_item_head_wait_info_tv"
	                android:layout_width="220dip"
	                android:layout_height="wrap_content"
	                android:text="00:00:00"
	                android:textSize="20sp"
	                android:textColor="@android:color/black"
	                android:layout_centerVertical="true"
	                android:layout_alignParentRight="true"
	                android:layout_marginRight="10dip"
	                android:background="@drawable/wait"
	                android:gravity="center"/>
	        </RelativeLayout>
	        <RelativeLayout android:id="@+id/experiment_new_main_item_head_blend_rl"
	            android:layout_width="wrap_content"
	            android:layout_height="54dip"
	            android:layout_centerHorizontal="true"
	            android:layout_below="@id/experiment_new_main_item_head_wait_rl">
	            <TextView android:id="@+id/experiment_new_main_item_head_blend_tv"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:text="@string/exp_blend"
	                android:textSize="20sp"
	                android:textColor="@android:color/white"
	                android:layout_centerVertical="true"/>
	            <TextView android:id="@+id/experiment_new_main_item_head_blend_info_tv"
	                android:layout_width="220dip"
	                android:layout_height="wrap_content"
	                android:text="00:00:00"
	                android:textSize="20sp"
	                android:textColor="@android:color/black"
	                android:layout_centerVertical="true"
	                android:layout_alignParentRight="true"
	                android:layout_marginRight="10dip"
	                android:background="@drawable/blend"
	                android:gravity="center"/>
	        </RelativeLayout>
	        <RelativeLayout android:id="@+id/experiment_new_main_item_head_magnet_rl"
	            android:layout_width="wrap_content"
	            android:layout_height="54dip"
	            android:layout_centerHorizontal="true"
	            android:layout_below="@id/experiment_new_main_item_head_blend_rl">
	            <TextView android:id="@+id/experiment_new_main_item_head_magnet_tv"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:text="@string/exp_magnetic"
	                android:textSize="20sp"
	                android:textColor="@android:color/white"
	                android:layout_centerVertical="true"/>
	            <TextView android:id="@+id/experiment_new_main_item_head_magnet_info_tv"
	                android:layout_width="220dip"
	                android:layout_height="wrap_content"
	                android:text="00:00:00"
	                android:textSize="20sp"
	                android:textColor="@android:color/black"
	                android:layout_centerVertical="true"
	                android:layout_alignParentRight="true"
	                android:layout_marginRight="10dip"
	                android:background="@drawable/magnet"
	                android:gravity="center"/>
	        </RelativeLayout>
	    </RelativeLayout>
	    <!-- 321dip  40 ->35dip-->
	    <RelativeLayout android:id="@+id/experiment_new_main_item_body_rl"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_below="@id/experiment_new_main_item_head_rl">
	        <RelativeLayout android:id="@+id/experiment_new_main_item_body_temp_rl"
	            android:layout_width="wrap_content"
	            android:layout_height="32dip">
	            <TextView android:id="@+id/experiment_new_main_item_body_temp_tv"
	                android:layout_width="164dip"
	                android:layout_height="fill_parent"
	                android:layout_centerVertical="true"
	                android:background="#314F67"
	                android:text="@string/exp_temp"
	                android:textSize="20sp"
	                android:textColor="@android:color/white"
	                android:gravity="center"
	                android:layout_marginLeft="2dip"/>
	            <TextView android:id="@+id/experiment_new_main_item_body_temp_info_et"
	                android:layout_width="104dip"
	                android:layout_height="fill_parent"
	                android:layout_centerVertical="true"
	                android:background="#314F67"
	                android:text="60"
	                android:textSize="20sp"
	                android:maxLength="3"
	                android:textColor="@android:color/white"
	                android:gravity="center"
	                android:layout_toRightOf="@id/experiment_new_main_item_body_temp_tv"
	                android:layout_marginLeft="2dip"/>
	            <TextView android:id="@+id/experiment_new_main_item_body_temp_unit_tv"
	                android:layout_width="60dip"
	                android:layout_height="fill_parent"
	                android:layout_centerVertical="true"
	                android:background="#314F67"
	                android:text="@string/exp_temp_unit"
	                android:textSize="20sp"
	                android:textColor="@android:color/white"
	                android:gravity="left|center_vertical"
	                android:layout_toRightOf="@id/experiment_new_main_item_body_temp_info_et"/>
	        </RelativeLayout>
	        <RelativeLayout android:id="@+id/experiment_new_main_item_body_vol_rl"
	            android:layout_width="wrap_content"
	            android:layout_height="32dip"
	            android:layout_marginTop="2dip"
	            android:layout_below="@id/experiment_new_main_item_body_temp_rl">
	            <TextView android:id="@+id/experiment_new_main_item_body_vol_tv"
	                android:layout_width="164dip"
	                android:layout_height="fill_parent"
	                android:layout_centerVertical="true"
	                android:background="#314F67"
	                android:text="@string/exp_vol"
	                android:textSize="20sp"
	                android:textColor="@android:color/white"
	                android:gravity="center"
	                android:layout_marginLeft="2dip"/>
	            <TextView android:id="@+id/experiment_new_main_item_body_vol_info_et"
	                android:layout_width="104dip"
	                android:layout_height="fill_parent"
	                android:layout_centerVertical="true"
	                android:background="#314F67"
	                android:text="550"
	                android:maxLength="4"
	                android:textSize="20sp"
	                android:textColor="@android:color/white"
	                android:gravity="center"
	                android:layout_toRightOf="@id/experiment_new_main_item_body_vol_tv"
	                android:layout_marginLeft="2dip"/>
	            <TextView android:id="@+id/experiment_new_main_item_body_vol_unit_tv"
	                android:layout_width="60dip"
	                android:layout_height="fill_parent"
	                android:layout_centerVertical="true"
	                android:background="#314F67"
	                android:text="ul"
	                android:textSize="20sp"
	                android:textColor="@android:color/white"
	                android:gravity="left|center_vertical"
	                android:layout_toRightOf="@id/experiment_new_main_item_body_vol_info_et"/>
	        </RelativeLayout>
	        <RelativeLayout android:id="@+id/experiment_new_main_item_body_speed_rl"
	            android:layout_width="wrap_content"
	            android:layout_height="32dip"
	            android:layout_marginTop="2dip"
	            android:layout_below="@id/experiment_new_main_item_body_vol_rl">
	            <TextView android:id="@+id/experiment_new_main_item_body_speed_tv"
	                android:layout_width="164dip"
	                android:layout_height="fill_parent"
	                android:layout_centerVertical="true"
	                android:background="#314F67"
	                android:text="@string/exp_blend_speed"
	                android:textSize="20sp"
	                android:textColor="@android:color/white"
	                android:gravity="center"
	                android:layout_marginLeft="2dip"/>
	            <TextView android:id="@+id/experiment_new_main_item_body_speed_info_tv"
	                android:layout_width="164dip"
	                android:layout_height="fill_parent"
	                android:layout_centerVertical="true"
	                android:background="#314F67"
	                android:text="6赫兹"
	                android:textSize="20sp"
	                android:textColor="@android:color/white"
	                android:gravity="center"
	                android:layout_toRightOf="@id/experiment_new_main_item_body_speed_tv"
	                android:layout_marginLeft="2dip"/>
	        </RelativeLayout>
	        <RelativeLayout android:id="@+id/experiment_new_main_item_body_hole_rl"
	            android:layout_width="wrap_content"
	            android:layout_height="32dip"
	            android:layout_marginTop="2dip"
	            android:layout_below="@id/experiment_new_main_item_body_speed_rl">
	            <TextView android:id="@+id/experiment_new_main_item_body_hole_tv"
	                android:layout_width="164dip"
	                android:layout_height="fill_parent"
	                android:layout_centerVertical="true"
	                android:background="#314F67"
	                android:text="@string/exp_hole"
	                android:textSize="20sp"
	                android:textColor="@android:color/white"
	                android:gravity="center"
	                android:layout_marginLeft="2dip"/>
	            <TextView android:id="@+id/experiment_new_main_item_body_hole_info_tv"
	                android:layout_width="164dip"
	                android:layout_height="fill_parent"
	                android:layout_centerVertical="true"
	                android:background="#314F67"
	                android:text="2"
	                android:textSize="20sp"
	                android:textColor="@android:color/white"
	                android:gravity="center"
	                android:layout_toRightOf="@id/experiment_new_main_item_body_hole_tv"
	                android:layout_marginLeft="2dip"/>
	        </RelativeLayout>
	        
	        <RelativeLayout android:id="@+id/experiment_new_main_item_body_switch_rl"
	            android:layout_width="wrap_content"
	            android:layout_height="42dip"
	            android:layout_marginTop="2dip"
	            android:layout_below="@id/experiment_new_main_item_body_hole_rl">
	            <TextView android:id="@+id/experiment_new_main_item_body_switch_tv"
	                android:layout_width="164dip"
	                android:layout_height="fill_parent"
	                android:layout_centerVertical="true"
	                android:background="#314F67"
	                android:text="@string/exp_temp_type"
	                android:textSize="20sp"
	                android:textColor="@android:color/white"
	                android:gravity="center"
	                android:layout_marginLeft="2dip"/>
	            <TextView android:id="@+id/experiment_new_main_item_body_switch_info_tv"
	                android:layout_width="164dip"
	                android:layout_height="fill_parent"
	                android:layout_centerVertical="true"
	                android:background="#314F67"
	                android:text="裂解加温"
	                android:textSize="20sp"
	                android:textColor="@android:color/white"
	                android:gravity="center"
	                android:layout_toRightOf="@id/experiment_new_main_item_body_switch_tv"
	                android:layout_marginLeft="2dip"/>
	        </RelativeLayout>
	    </RelativeLayout>
	    <TextView android:id="@+id/experiment_new_main_item_top_name_tv"
	        android:layout_width="fill_parent"
	        android:layout_height="42dip"
	        android:text="步骤一"
	        android:textSize="25sp"
	        android:textColor="@android:color/white"
	        android:background="#505E8D"
	        android:gravity="center"
	        android:layout_margin="2dip"
	        android:layout_alignParentBottom="true"/>
    </RelativeLayout>
</RelativeLayout>
